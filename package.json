{
  "name": "illusory",
  "version": "2.0.3",
  "description": "Seamlessly morph one element into another.",
  "keywords": [
    "illusory",
    "morph",
    "transform",
    "ramjet",
    "shared element",
    "morph element",
    "transition",
    "animation",
    "element transition"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.js",
  "types": "dist/types",
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "build": "rollup --config",
    "dev": "npm run build -- --watch",
    "dev:link": "npm link",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justintaddei/illusory.git"
  },
  "author": "Justin Taddei <justin@justintaddei.com> (https://justintaddei.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justintaddei/illusory/issues"
  },
  "homepage": "https://github.com/justintaddei/illusory#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/jest": "^25.2.3",
    "eslint": "^7.5.0",
    "jest": "^29.6.1",
    "live-server": "^1.2.1",
    "prettier": "^1.19.1",
    "rollup": "^2.23.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "rematrix": "^0.7.0"
  }
}
